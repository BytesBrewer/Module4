<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dzone.module4">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NsgActivity"
            android:label="National Security Guard" />
        <activity
            android:name=".NsgHistoryActivity"
            android:label="History" />
        <activity
            android:name=".NsgMissionActivity"
            android:label="Mission" />
        <activity
            android:name=".NsgSelActivity"
            android:label="Selection and Training" />
        <activity
            android:name=".NsgRegDepActivity"
            android:label="Regional Deployment" />
        <activity android:name=".NsgRankActivity"></activity>
    </application>

</manifest>